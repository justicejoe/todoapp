{
  "name": "face-sentiment-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "pm2 startOrRestart process.yml",
    "develop": "DOTENV_CONFIG_PATH=.env npx nodemon -r @babel/register -r dotenv/config src/app.js",
    "pm2": "pm2",
    "pause": "pm2 stop process.yml",
    "restart": "pm2 restart process.yml",
    "logs": "pm2 logs",
    "setup:local": "cp .env.local .env && cp process.dev.yml process.yml && cp docker-compose.dev.yml docker-compose.yml",
    "setup:dev": "cp .env.dev .env && cp process.dev.yml process.yml && cp docker-compose.dev.yml docker-compose.yml"
  },
  "dependencies": {
    "@babel/core": "^7.6.2",
