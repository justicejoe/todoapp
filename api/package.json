    "async": "^3.1.1",
    "axios": "^0.19.2",
    "bluebird": "^3.7.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "config": "^3.2.3",
    "connect-redis": "^4.0.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "express-flash": "^0.0.2",
    "express-jwt": "^5.3.1",
    "express-session": "^1.17.0",
    "graphql": "^14.5.8",
